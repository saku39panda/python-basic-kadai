        "def calc(price, tax):\n",
        "  tax_amount = price * (tax / 100)\n",
        "  total = price + tax_amount\n",
        "\n",
        "  return total\n",
        "\n",
        "price = 110\n",
        "tax = 10\n",
        "\n",
        "total_price = calc(price, tax)\n",
        "print(total_price)\n"
